@model Hospital
<h1>Detalles</h1>

<a asp-controller="Hospitales" asp-action="Index">Volver</a>

<ul class="list-group">
	<li class="list-group-item">
		ID: @Model.IdHospital
	</li>
	<li class="list-group-item">
		Nombre: @Model.Nombre
	</li>
	<li class="list-group-item">
		Direccion: @Model.Direccion
	</li>
	<li class="list-group-item">
		Teléfono: @Model.Telefono
	</li>
	<li class="list-group-item">
		Camas: @Model.Camas
	</li>
</ul>